{
  "name": "bookshelf",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bookshelf.lol/",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/dialog": "^0.10.5",
    "@reach/menu-button": "^0.10.5",
    "@reach/tabs": "^0.10.5",
    "@reach/tooltip": "^0.10.5",
    "@reach/visually-hidden": "^0.10.4",
    "bootstrap": "^4.5.0",
    "codegen.macro": "^4.0.0",
    "debounce-fn": "^4.0.0",
    "faker": "^4.1.0",
    "history": "^5.0.0",
    "match-sorter": "^4.1.0",
    "msw": "0.19.5",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-error-boundary": "^2.2.3",
    "react-icons": "^3.10.0",
    "react-query": "2.4.15",
    "react-query-devtools": "^2.3.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^3.4.1",
    "stop-runaway-react-effects": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "10.4.6",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^12.0.11",
    "cross-env": "^7.0.2",
    "cypress": "^4.10.0",
    "cypress-hmr-restarter": "^1.2.1",
    "eslint-plugin-cypress": "^2.11.1",
    "husky": "^4.2.5",
    "inquirer": "^7.3.2",
    "is-ci": "^2.0.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.1.0",
    "jest-watch-typeahead": "^0.6.0",
    "netlify-plugin-cypress": "^1.3.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "resolve": "^1.17.0",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.2",
    "whatwg-fetch": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build --profile",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --watch --runInBand",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "cross-env BROWSER=none start-server-and-test start http://localhost:3000 cy:open",
    "test:e2e:run": "start-server-and-test serve http://localhost:8811 cy:run",
    "serve": "serve --no-clipboard --single --listen 8811 build",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "format": "prettier --write \"**/*.+(js|json|css|md|mdx|html)\"",
    "setup": "node ./scripts/setup && node ./scripts/track-branches.js",
    "validate-exercises": "node ./scripts/validate-exercises",
    "validate": "npm run validate-exercises && npm-run-all --parallel lint test:coverage build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
